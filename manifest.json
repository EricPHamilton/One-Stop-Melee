{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "icons": { "16": "icons/Shine16.png",
             "48": "icons/Shine48.png",
             "128": "icons/Shine128.png"
           },

  "browser_action": {
    "default_icon": "icons/Shine128.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
